/*userinfo*/
#user_info {position:relative;height:25px;padding:10px 0 0 10px;background:#eaeaea;z-index:1001;min-width:1280px;border-top:solid 2px #5499de;border-bottom:solid 1px #d9d8d8;clear:both;color:#404040;}
.topnotice {float:left;width:400px;font:bold 11px '돋움';padding:2px 0 0 0;}
.topnotice a {background:#ffffff;border:solid 1px #d9d8d8;padding:5px 5px 3px 5px;color:#484848;}
.topnotice a:hover {background:#f9f9f9;}
.topnotice a.on {color:#ffffff;background:#5499de;border:solid 1px #3c7dbe;}
.block_user {padding:0px 20px 0 0;float:right;line-height:16px;}
.block_user span {font-weight:bold;}
.block_user img {vertical-align:middle;}



