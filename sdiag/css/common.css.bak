/*스크롤 테이블*/
.index_tbl {width:100%;border:solid 1px #dcdcdc;border-top:0px;float:left;}
.scroll_list {width:100%;clear:both;border-collapse:collapse;margin:0;clear:both;}
.scroll_list thead tr {color:#393939;background:#f8f8f8;vertical-align:middle;height:25px;font:bold 12px "돋움";margin:0;}
.scroll_list thead tr th {height:20px;border-top: 2px solid #5585c1;border-bottom:solid 1px #dcdcdc;vertical-align: middle;border-collapse:collapse;padding:10px 0;margin:0;}
.scroll_list tbody tr {font:normal 12px "돋움";vertical-align: middle;color:#393939;}
.scroll_list tbody tr th {height:40px;text-align: center;vertical-align: middle;padding:10px 0;border-collapse:collapse;}
.scroll_list tbody tr td {height:30px;text-align: center;vertical-align: middle;padding:10px 0;border-collapse:collapse;border-bottom:solid 1px #dcdcdc;color:#676767;}
.scroll_list tbody tr td p {display:block;}
.scroll_list tbody tr td.cellNone {height:347px;}
.scroll_list tbody tr td.cellNone:hover {background:#fff;}
.btn_details {border:solid 1px #e9e9e9;padding:5px 5px 5px 25px; background:#f3f3f3 url(/img/btn_details.png) no-repeat 5px;color:#393939;font:bold 12px "돋움";border-radius:3px;}

