@charset "utf-8";
/* CSS Document */

html,body { margin:0; padding:0; color:#666;}
body {width:100%;height:100%;min-width:1280px;color:#666; font-size:12px;font-family:돋움, Dotum, Verdana, sans-serif; -webkit-text-size-adjust:none;}

body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td{ margin:0; padding:0; }
table { border-collapse:collapse; border-spacing:0; }
caption {visibility:hidden; font-size:0; width:0; height:0; line-height:0}

fieldset,img,iframe { border:0; }
button { margin:0; padding:0; border:none; cursor:pointer;vertical-align:top; font-family:돋움, Dotum, Verdana, sans-serif; }
h1,h2,h3,h4,h5,h6{ font-size:100%; font-weight:normal; font-family:돋움, dotum, Verdana, sans-serif; }
h4 {display:none;}
li,dt,dd {list-style:none;}
li,dt,dd,img,p {display:inline;padding:0;margin:0;border:0;}
input,select { vertical-align:middle; }
input,textarea,select{ font-family:inherit; font-size:inherit; font-weight:inherit; }
textarea {resize:none;}
a {text-decoration:none;}

/***layout***/
#header {position:relative;height:55px;background:#ffffff;z-index:1000;width:100%;min-width:1280px;}
#maincnt {height:auto;width:100%;min-width:1280px;padding:0;margin:0 auto;}
#mainwarp {width:100%;min-width:1280px;margin:0;}
#maincnt_L {position:relative;float:left;width:290px;left:-190px;display:inline-block;height:outo;min-height:848px;padding:5px 10px;}
#maincnt_R {position:relative;height:200px;margin-top:10px;border:1px solid #bdbdbd;background:#fff;text-align:center}
#cnt {position:relative;height:auto;width:100%;min-width:1280px;padding-right:0px;;background:#ffffff url('/img/bg.jpg') repeat-y;}
#cnt_L {position:relative;float:left;width:290px;display:inline-block;height:outo;min-height:848px;}
#cnt_R {width:75%;float:left;left:290px;*left:0;margin:20px 0 0 20px;}
#footer {padding-top:10px;height:20px;background:#efefef;z-index:9999;width:100%;min-width:1280px;bottom:0;clear:both;text-align:center;}
/* UI Object */
#container{position:relative;*display:inline-block;_width:100%;/*padding-right:200px;padding-left:200px;*/}
#container:after{display:block;clear:both;content:''}
.snb{float:left;position:relative;left:-190px;width:290px;margin-right:-290px;text-align:center;margin-top:5px;}
#content{position:relative;float:left;width:99%;left:120px;margin:20px 0 0 0;}
/* .aside{float:left;position:relative;width:65px;margin-left:-65px;left:200px;background:#5499de;margin-top:20px;text-align:center;color:#ffffff;} */
.aside{float:right;position:absolute;width:65px;right:10px;top:100px;background:#5499de;text-align:center;color:#ffffff;} 
.aside ul{font-weight:bold;text-align:center;}
.aside ul li {position:relative;text-align:center;border-bottom:solid 1px #478acc;height:70px;display:block;}
.aside ul li.cell1 {background:#434343;height:80px;}
.aside ul img {display:block;text-align:center;}
.aside ul li span.icon_new {position:absolute;top:9px;right:-5px;width:17px;height:18px;background:url(/img/icon_new.png) no-repeat;}
#footer{width:100%}

/*topmenu*/
#header h1 {float:left;width:300px;height:40px;background:url('/img/top_logo.png') no-repeat 0 0;text-indent:-9999px;margin:15px 0 0 10px;}
ul.lnb_menu {position:absolute;padding:21px 20px 0 0;float:right;right:0;}
ul.lnb_menu li {padding:0 2px 0 0;dispaly:inline-block;}
.topmenu {font:bold 13px '돋움';color:#616161;text-decoration:none;cursor:pointer;line-height:30px;;
display:block;/*opera*/display:inline-block;/*ie*/display:-moz-inline-box;/*ff*/padding:0px 5px 0px 0px;vertical-align:middle;white-space:nowrap; margin:0px 0px;/*safari*/vertical-align:top;
}
.topmenu span {font:bold 13px '돋움';-moz-user-select:none;*selector-dummy:expression(this.hideFocus=true);text-transform:uppercase;height:30px;display:inline-block;padding:0px 20px 0px 25px;line-height:20px;
}
a.topmenu:hover {color:#5499de;margin:0px 0px;/*safari*/}
a.topmenu:hover span {cursor:pointer;}
/* (selected) */
.topmenuSel {color:#5499de;border-bottom:solid 4px #5499de;}


/*all_menu*/
.all_menu {position:absolute;background:#ffffff;z-index:9999;top:55px;box-shadow:0 10px 10px -5px #f4f4f4;border-top:solid 2px #5499de;display:none;}
.menuwarp {min-width:100px;margin:0 auto;padding:5px 0 10px 0;text-align: center;}
.menuwarp ul {display:inline-block;;padding:0 10px;min-width:100px;border-right:solid 1px #e7e7e7;text-align: left;vertical-align: top;}
.menuwarp ul:last-child {border-right:0px;}
.menuwarp ul h2 {font:bold 12px '돋움';color:#5091d3;padding:10px 0 0 10px;}
.menuwarp ul li {color:#5a5a5a;display:block;padding:3px 10px; letter-spacing:-1pt;}
.menuwarp ul li a {color:#5a5a5a;padding:0 0 0 10px;background:url(/img/icon_arow5.png) left no-repeat;}
.menuwarp ul li a:hover {text-decoration:underline;}
.menuwarp ul li span a {background:none;font:bold 12px '돋움';padding:12px 0 4px 0;color:#5a5a5a;display:block;}
.menuwarp ul li span a:hover {text-decoration:none;}


