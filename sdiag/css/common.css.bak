/*체크박스*/
.ck_button label {
	display:inline-block;
}
.ck_button label span {
	text-align:left;
	padding:0px 10px 0px 30px;
	display:block;
	width:auto;
	height:25px;
	border-radius:0;
	background:url(/img/check_off.png) no-repeat left center;
	vertical-align:middle;
	line-height:25px;
	top:-15px;
	cursor:default;
}
.ck_button label input {
	position:absolute;
	opacity:0;
	border:0;
}
.ck_button input:checked + span {
	background:url(/img/check_on.png) no-repeat left center;
	color:#393939;
}
/*라디오버튼*/
