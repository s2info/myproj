<!-- 공지사항 update 
<update id="notice.noticeRegisterUpdate" parameterClass="communitynoticeVO">
	<![CDATA[
	UPDATE "NOTICE_INFO"
		SET n_type= #n_type#,
			title= #title#,
			contents= #contents#,
			status_cd= #status_cd#, 
       		upd_date= NOW(),
       		is_popup= #is_popup#
	 WHERE 1 = 1
	   AND sq_no = #sq_no#
	]]>
</update> 	

<select id="community.getMaxSqNo" resultClass="communitynoticeVO">
	SELECT MAX(sq_no) AS sq_no FROM "NOTICE_INFO"
</select>

